{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
        "/*": ["./public/*"],
        "/frappe-charts.min.esm.js": [
          "./node_modules/frappe-charts/dist/frappe-charts.min.esm.js"
        ]
    },
    "noEmit": true,
    "allowJs": true,
    "checkJs": true,
    "strict": true
  },
  "include": [
    "**/*.d.ts",
    "**/*.js"
  ],
  "exclude": [
    "out/*",
    "src/isomorphic/object-map.js"
  ]
}
